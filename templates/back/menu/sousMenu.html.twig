<ul>
    {% for menuPage in menuPages %}
        {% if isPublie(menuPage.page) %}
            <li>
                <a href="#"><span class="menuPage" id="{{ menuPage.id }}"></span><span class="page" id="{{ menuPage.page.id }}"></span>{{ menuPage.page.titre }}</a>
                {{ sousMenu(menuPage.page.id, 'back')|raw }}
            </li>
        {% endif %}
    {% endfor %}
</ul>