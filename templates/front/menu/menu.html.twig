{% for menuPage in menuPages %}
    {% if menuPage.pageParent is empty and isPublie(menuPage.page) %}
        <li {% if isParent(menuPage.page.id) %}class="parent"{% endif %}>
            <a href="{{ path('voirPage', {'url':menuPage.page.seo.url}) }}">{{ menuPage.page.titre }}</a>
            {{ sousMenu(menuPage.page.id, 'front')|raw }}
        </li>
    {% endif %}
{% endfor %}